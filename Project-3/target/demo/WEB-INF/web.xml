<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
    <welcome-file-list>
        <welcome-file>authentication.html</welcome-file>
    </welcome-file-list>
  <display-name>Project-3</display-name>
  <servlet>
    <servlet-name>ClientServlet</servlet-name>
    <servlet-class>com.example.demo.ClientServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>ClientServlet</servlet-name>
    <url-pattern>/ClientServlet</url-pattern>
</servlet-mapping>
<servlet>
        <servlet-name>DataEntryServlet</servlet-name>
        <servlet-class>com.example.demo.DataEntryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataEntryServlet</servlet-name>
        <url-pattern>/DataEntryServlet</url-pattern>
    </servlet-mapping>
  <servlet>
  <servlet-name>AuthenticateServlet</servlet-name>
  <servlet-class>com.example.demo.AuthenticateServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>AuthenticateServlet</servlet-name>
  <url-pattern>/AuthenticateServlet</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>AccountantServlet</servlet-name>
    <servlet-class>com.example.demo.AccountantServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>AccountantServlet</servlet-name>
    <url-pattern>/AccountantServlet</url-pattern>
</servlet-mapping>
<servlet>
  <servlet-name>RootServlet</servlet-name>
  <servlet-class>com.example.demo.RootServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>RootServlet</servlet-name>
  <url-pattern>/RootServlet</url-pattern>
</servlet-mapping>
</web-app>